				.errors
				form(id='rpfForm' name="rpfForm")
					h5 Planning your meeting of event Free quotation - Please fill out the form below
					section
						.form-group
							label Select Location
							.input-showtext
								button(name='button', type='button', value='空室を検索') Cities, Hotels
								.popup-menu
									.popup-content-input

										ul.cities
											label Cities
											li
												span(target='Hokkaido', names='Hokkaido') 1名
											li
												span(target='Okinawa', names='Okinawa') 2名
											li
												span(target='Tochigi', names='Tochigi') 3名
											li
												span(target='Tokyo', names='Tokyo') 4名
											label Hotels
											ul.hotels
												li
													span(target='Hokkaido', names='Hokkaido') 1名
												li
													span(target='Okinawa', names='Okinawa') 2名
												li
													span(target='Tochigi', names='Tochigi') 3名
												li
													span(target='Tokyo', names='Tokyo') 4名
					label First Name
					.input-showtext
						input.form-control(id='fname' name= "fname" type='text', placeholder='First Name' )
					label Last Name
					.input-showtext
						input.form-control(type='text', placeholder='Last Name' name='lname')
					label Company Name
					.input-showtext
						input.form-control(type='text', placeholder='Company Name' name='lname')
					label Email Address
					.input-showtext
						input.form-control(type='text', placeholder='Email Address' name='details')
					label Phone Number
					.input-showtext
						input.form-control(type='number', placeholder='Phone Number' name='mobile')
					h5 Meeting / Event Information
					label Event / Meeting Name
					.input-showtext
						input.form-control(type='text', placeholder='Event / Meeting Name' name='details')
					label Nr. Attendees
					.input-showtext
						input.form-control(type='number', placeholder='Nr. Attendees' name='mobile')

					label Start Date
					.input-showtext
						input.form-control(type='text', placeholder='Start Date' name='details')
					label End Date
					.input-showtext
						input.form-control(type='number', placeholder='End Date' name='mobile')
					label Guest Room
					.input-showtext
						input.form-control(type='text', placeholder='Guest Room' name='details')
					label Duration
					.input-showtext
						input.form-control(type='number', placeholder='Duration' name='mobile')
					section
						.form-group
							label Type of Event
							.input-showtext
								button(name='button', type='button', value='空室を検索') Trade show
								.popup-menu
									.popup-content-input
										ul.cities
											li
												span(target='Hokkaido', names='Hokkaido') Party
											li
												span(target='Okinawa', names='Okinawa') Meeting
											li
												span(target='Tochigi', names='Tochigi') Conference
											li
												span(target='Tokyo', names='Tokyo') Others
					section
						.form-group.food-beverage
							.form-checkbox
								label.form-label Check applicable food and beverage needs
								.checkbox-style
									label.checkbox
										input.form-control(type='checkbox')
										span
											i.icon.sprite-checked_sp
										|	Breakfast
								.checkbox-style
									label.checkbox
										input.form-control(type='checkbox')
										span
											i.icon.sprite-checked_sp
										|	Lunch
								.checkbox-style
									label.checkbox
										input.form-control(type='checkbox')
										span
											i.icon.sprite-checked_sp
										|	Dinner
								.checkbox-style
									label.checkbox
										input.form-control(type='checkbox')
										span
											i.icon.sprite-checked_sp
										|	Reception
								.checkbox-style
									label.checkbox
										input.form-control(type='checkbox')
										span
											i.icon.sprite-checked_sp
										|	Others
					label Check applicable food and beverage needs
					.input-showtext
						input.form-control(type='number', placeholder='Duration' name='mobile')
					label Please tell us about your other / Audio Visual requirements:
					.input-showtext
						input.form-control(type='number', placeholder='Duration' name='mobile')
					h5 Free Quotation - We will call you within 24H
					h5 By submiting this RFP, you are agreeing with our Terms and Conditions and Privacy Statement.
					.find-button
						button(name='button', type='button', value='空室を検索' onclick='FE.global.validateForm()') Submit my RPF
