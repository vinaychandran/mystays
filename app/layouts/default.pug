doctype html
html.no-js(lang='')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(rel='shortcut icon', href='assets/images/favicon.ico?v=1.0')
    title OE web scaffolding

    // build:css assets/styles/vendor.css
    // bower:css
    // endbower
    // endbuild

    // build:css assets/styles/main.css
    link(rel='stylesheet', href='assets/styles/main.css')
    link(rel='stylesheet', href='assets/styles/components/modal.css')
    <!-- link(rel='stylesheet', href='https://cdn.jsdelivr.net/foundation/5.5.0/css/foundation.css') -->
